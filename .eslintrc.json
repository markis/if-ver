{
  "extends": "eslint:recommended",
  "env": {
    "browser": false,
    "commonjs": true,
    "es6": false,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": ["error", "only-multiline"],
    "comma-style":["error", "last"],
    "quotes": ["error", "single", {"avoidEscape": true}],
    "semi": "error"
  }
}